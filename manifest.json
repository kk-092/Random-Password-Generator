{
    "manifest_version": 3,
    "name": "Secure Password Generator",
    "version": "1.0",
    "description": "Generate secure random passwords.",
    "icons": {
        "256": "icons/icon256.png",
        "128": "icons/icon128.png",
        "48": "icons/icon48.png",
        "16": "icons/icon16.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "256": "icons/icon256.png",
            "128": "icons/icon128.png",
            "48": "icons/icon48.png",
            "16": "icons/icon16.png"
        }
    },
    "permissions": [
        "storage"
    ]
  }